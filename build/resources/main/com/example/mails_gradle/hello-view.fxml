<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.mails_gradle.MailController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <ComboBox fx:id="mails" layoutX="7.0" layoutY="33.0" prefWidth="200" promptText="MAILS"/>

    <ComboBox fx:id="texts" layoutX="14.0" layoutY="33.0" prefWidth="200" promptText="TEXT"/>

    <TextField fx:id="pass_field" layoutX="1" layoutY="50" prefWidth="250" promptText="Введите ключ шифрования"/>

    <Label fx:id="Text" layoutX="14.0" layoutY="33.0" prefWidth="200"/>
    <Button text="Рассылка" onAction="#onHelloButtonClick"/>
</VBox>
